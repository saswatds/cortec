{
  "$schema": "./node_modules/nx/schemas/nx-schema.json",
  "defaultBase": "main",
  "targetDefaults": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "{projectRoot}/dist"
      ],
      "cache": true
    },
    "lint": {
      "cache": true,
      "dependsOn": [
        "build"
      ]
    },
    "test": {
      "cache": true,
      "dependsOn": [
        "build"
      ]
    },
    "depcheck": {}
  },
  "plugins": []
}